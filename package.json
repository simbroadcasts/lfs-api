{
  "name": "lfs-api",
  "version": "1.0.0",
  "type": "module",
  "description": "Query the LFS API in your NodeJS projects",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PButcher/lfs-api.git"
  },
  "author": "Peter Butcher <pbutcher93@gmail.com> (https://pbutcher.uk)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PButcher/lfs-api/issues"
  },
  "homepage": "https://github.com/PButcher/lfs-api#readme",
  "scripts": {
    "dist": "tsc -p ./",
    "dev": "ts-node-dev --respawn ./src/index.ts -d",
